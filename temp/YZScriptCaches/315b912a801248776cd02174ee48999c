require(["zenjs/util/ua","zenjs/util/args"],function(o,i){var n=$(".js-mask"),e=$(".js-trigger-slider"),a=$(".js-menu-list"),s=$(".js-bg-mask");e.on("click",function(){a.addClass("slider-down"),s.show()}),s.on("click touchend",function(o){0==$(o.target).parents(".js-menu-list").length&&(a.removeClass("slider-down"),s.hide())});var t=o.isIOS(),c=o.isAndroid(),r=o.isWeixin();$(".js-download").on("click",function(){var o=i.get("from_source");if(t&&(window.location.href=o?"http://a.app.qq.com/o/simple.jsp?pkgname=com.qima.mars&ckey=CK1346954523467":"http://a.app.qq.com/o/simple.jsp?pkgname=com.qima.mars"),c)if(r)n.show();else{var e=i.get("promote_source");if(e)return window.location.href="http://app.yzcdn.cn/"+e+".apk";if(o)return window.location.href="http://a.app.qq.com/o/simple.jsp?pkgname=com.qima.mars&ckey=CK1346954523467";window.location.href="http://dl.koudaitong.com/mjb_A_youzan.apk"}}),n.on("click",function(){n.hide()})}),define("main",function(){});