require(["zenjs/util/ua","zenjs/util/args"],function(n,i){var o=Backbone.View.extend({initialize:function(){this.isLogined=!!window._global.buyer_id&&window._global.buyer_id.length>0},events:{"click .js-download-btn":"onDownloadClick","click .js-mask":"onMaskClick"},onMaskClick:function(){$(".js-mask").hide()},onDownloadClick:function(o){var e=$(o.target),t=e.data("href"),a=i.get("promote_source"),c=n.isAndroid(),l=n.isWeixin();window.Logger&&Logger.log({fm:"click",link:encodeURIComponent(window.location.href)}).then(function(){if(a&&c)return l?$(".js-mask").show():location.href="http://app.yzcdn.cn/"+a+".apk",!1;location.href=t})}});window.app=new o({el:$("body")})}),define("main",function(){});