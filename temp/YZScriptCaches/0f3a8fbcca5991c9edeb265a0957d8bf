require([],function(){var i=$("#shop_item_empty"),o=$(".search-input"),n=$(".close-icon"),a=function(){$(".block-order").show(),n.addClass("hide"),i.hide()};$(".js-search").on("input",function(i){n.removeClass("hide"),$(i.target).val()||n.addClass("hide")}),$(".js-search-btn").on("click",function(){var i=o.val();location.href=window._global.wap_url.wap+"/showcase/physicalstore?kdt_id="+window._global.kdt_id+"&keyword="+i}),n.on("click",function(){o.val(""),a()})}),define("main",function(){});