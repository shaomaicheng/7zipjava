define("wap/pages/fx/tabber",[],function(){var t=window,e=t.document,i=$(e),n=function(t){return this.el=t.el,this.$el=$(this.el),this.$content=$(t.content),this.current=t.current||this.$el.eq(0).attr("href"),this._init(),this._bindEvents(),this};return n.prototype={_init:function(){this.activeItem(this.current)},_bindEvents:function(){i.delegate(this.el,"click",this.tabberClickHandler.bind(this))},tabberClickHandler:function(t){var e=$(t.target),i=e.attr("href")||e.data("target");this.activeItem(i),t.preventDefault()},activeItem:function(t){var e=this.$el,i=this.$content;e.find(".active").removeClass("active"),i.removeClass("active"),$(t).addClass("active"),e.find('[href="'+t+'"]').addClass("active")}},n}),define("wap/pages/fx/tab_slider",[],function(){var t=$(".js-trigger-slider"),e=$(".js-menu-list"),i=$(".js-bg-mask");t.on("click",function(){e.addClass("slider-down"),i.show()}),i.on("click touchend",function(t){0==$(t.target).parents(".js-menu-list").length&&(e.removeClass("slider-down"),i.hide())})}),require(["wap/pages/fx/tabber","wap/pages/fx/tab_slider"],function(t){$(".fx-banner").append($("#video-section").html());var e=$(".video-section"),i=$(".js-watch-vidio")[0],n=$(".video-block")[0];$("body").on("click",function(t){if(t.target===i)t.stopPropagation(),e.css({left:0});else{if(t.target===n)return;n&&n.pause(),e.css({left:"-1000px"})}}),new t({el:".js-tabber",content:".js-content",current:"#fuwu_become"})}),define("main",function(){});