!function(){var o;$("body").on("click",".js-submit",function(e){var n=$(".js-phone"),t=$(".js-submit"),i=n.val();o||$._ajax({url:"/trade/secure/verifyPhone.json",type:"POST",dataType:"json",data:{phone:i,kos_token:_global.kos_token},beforeSend:function(){o=!0,t.attr("disabled","disabled")},success:function(o){if(0!=o.code)return void motify.log(o.msg);motify.log("验证成功，跳转中",1e4),setTimeout(function(){location.href=window._global.redirect_uri+"&phone="+i},500)},error:function(){motify.log("验证失败，请重试")},complete:function(){t.removeAttr("disabled"),o=!1}})})}(),define("main",function(){});