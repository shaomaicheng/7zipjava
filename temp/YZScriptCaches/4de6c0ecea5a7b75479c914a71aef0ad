define("text!wap/showcase/empty_recommend/recommend.html",[],function(){return'<div>\n\t<p class="recommend-tips c-gray-dark font-size-14 center">看了该宝贝的人还看了</p>\n\t<div class="js-recommend-goods-list"></div>\n</div>\n'}),require(["zenjs/class","text!wap/showcase/empty_recommend/recommend.html"],function(e,n){new(e.extend({init:function(){var e=$(".js-recommend-container");$._ajax({url:"/v2/showcase/goods/recommendgoodslist.json",type:"GET",dataType:"json",data:{count:10,kdt_id:window._global.kdt_id},success:function(o){var d=o.code,s=o.data||{};if(0==d&&s.html){var i=$(n);i.find(".js-recommend-goods-list").append(s.html),i.find(".js-goods-list-loading").remove(),i.find(".js-goods-list").css("visibility","visible"),$&&$.fn&&$.fn.lazyload&&i.find(".js-goods-lazy").goodsLazyLoad(),e.append(i)}}})}}))}),define("main",function(){});